            <input type="text" name="notable_songs" placeholder="Notable Songs" />
            <input type="text" name="cover" value="https://img.freepik.com/premium-vector/vector-3d-realistic-blank-cd-dvd-with-paper-cover-case-box-set-closeup-isolated-white-background-design-template-mockup-cd-packaging-copy-space-top-view_153563-2891.jpg" />
            <input type="number" name="likes" placeholder="Likes 1" />
            <button type="submit">Create Album</button>
            
        </form>
    </div>
    
